<UserControl x:Class="Me.Amon.Apwd.Views.Mopt.Aopt"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="470" d:DesignWidth="640">

    <Grid x:Name="LayoutRoot" Height="470" Width="640">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="30" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="160" />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <ListBox Grid.Column="0" Margin="10,10,10,10" Name="listBox1">
            <ListBoxItem Content="系统" />
            <ListBoxItem Content="数据" />
            <ListBoxItem Content="用户" />
        </ListBox>
        <Grid Grid.Column="1" Margin="10,10,10,10" Name="grid1" />
        <Button Content="取消" Grid.Column="1" Grid.Row="1" Height="23" HorizontalAlignment="Right" Margin="0,0,12,0" Name="BtOk" VerticalAlignment="Top" Width="75" Click="BtOk_Click" />
        <Button Content="确定" Grid.Column="1" Grid.Row="1" Height="23" HorizontalAlignment="Right" Margin="0,0,93,0" Name="BtNo" VerticalAlignment="Top" Width="75" Click="BtNo_Click" />
    </Grid>
</UserControl>
