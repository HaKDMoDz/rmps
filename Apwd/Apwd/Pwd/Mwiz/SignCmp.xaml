<UserControl x:Class="Me.Amon.Apwd.Views.Mpwd.Mwiz.SignCmp"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="23" d:DesignWidth="400">
    
    <Grid x:Name="LayoutRoot" Background="White">
        <TextBox Name="TbData" Height="23" Margin="0,0,24,0" VerticalAlignment="Top" GotFocus="TbData_GotFocus" />
        <Button Name="SignBtn" Height="18" HorizontalAlignment="Right" Margin="0,2,0,0" VerticalAlignment="Top" Width="18" Click="SignBtn_Click" ToolTipService.ToolTip="选项">
            <Image Name="SignImg" Source="/img/att/sign.png" />
        </Button>
    </Grid>
</UserControl>
