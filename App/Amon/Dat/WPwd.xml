      <Item Id="toolbar-visible" Type="toggle" Text="显示工具栏" ActionId="toolbar-visible">
        <Stroke Key="^ T" Memo="切换工具栏显示状态"/>
      </Item>
      <Item Id="echobar-visible" Type="toggle" Text="显示状态栏" ActionId="echobar-visible">
        <Stroke Key="^ E" Memo="切换状态栏显示状态"/>
      </Item>
      <Item Id="findbar-visible" Type="toggle" Text="显示查找栏" ActionId="findbar-visible">
